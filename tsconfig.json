{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es2022",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "types": ["webpack/module"],
    "lib": ["es2015", "dom", "ES2022"],
  },
}
